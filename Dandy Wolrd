local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/UserR16/DandyWorld/refs/heads/main/Source"))()
local LibraryESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/RGV-devid/Scripts/refs/heads/main/ESP"))()

-- Create Window
local Window = Library:AddWindow({
	title = {"YOUHUB", "Dandy's World"},
	theme = {
		Accent = Color3.fromRGB(255, 255, 0)
	},
	key = Enum.KeyCode.RightControl,
	default = true
})

local Tab = {
Main = Window:AddTab("Main", {default = true}),
Players = Window:AddTab("Players", {default = false}),
ESP = Window:AddTab("ESP", {default = false})
}

local Main = Tab.Main:AddSection("Main", {default = true})
Main:AddToggle("Auto SkillChecks", {flag = "ASC_flag", default = false}, function(v)
if v then
SkillCheck = game:GetService("RunService").RenderStepped:Connect(function()
local ScreenGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("ScreenGui")
if not ScreenGui then
return
end
local Menu = ScreenGui:FindFirstChild("Menu")
if not Menu then
return
end
local SkillCheckFrame = Menu:FindFirstChild("SkillCheckFrame")
if not SkillCheckFrame then
return
end
if SkillCheckFrame:FindFirstChild("GoldArea") then
SkillCheckFrame:FindFirstChild("GoldArea").Size = UDim2.new(0,1000,0,1000)
SkillCheckFrame:FindFirstChild("GoldArea").Transparency = 1
end
end)
else
SkillCheck:Disconnect()
end
end)

Main:AddToggle("Auto SkillCheck", {flag = "ASC_flag", default = false}, function(v)
if v then
SkillCheck = game:GetService("RunService").RenderStepped:Connect(function()
local ScreenGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("ScreenGui")
if not ScreenGui then
return
end
local Menu = ScreenGui:FindFirstChild("Menu")
if not Menu then
return
end
local SkillCheckFrame = Menu:FindFirstChild("SkillCheckFrame")
if not SkillCheckFrame then
return
end
SkillCheckFrame:GetPropertyChangedSignal("Visible"):Connect(function()
if SkillCheckFrame.Visible then
local Marker = SkillCheckFrame:FindFirstChild("Marker")
local GoldArea = SkillCheckFrame:FindFirstChild("GoldArea")
if Marker and GoldArea then
task.spawn(function()
while SkillCheckFrame.Visible do
local MarkerPos = Marker.AbsolutePosition
local GoldAreaPos = GoldArea.AbsolutePosition
local GoldAreaSize = GoldArea.AbsoluteSize
if MarkerPos.X >= GoldAreaPos.X and MarkerPos.X <= (GoldAreaPos.X + GoldAreaSize.X) then
game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Space, false, game)
game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Space, false, game)
break
end
task.wait(0.01)
end
end)
end
end
end)
end)
else
SkillCheck:Disconnect()
end
end)
Main:AddToggle("Auto Return to Elevator", {flag = "ARTE_flag", default = false}, function(v)
_G.AutoReturnToElevator = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoReturnToElevator then
if workspace.Elevators.Elevator.Opened == true then
if game:GetService("Players").LocalPlayer:WaitForChild("Character") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Elevators.Elevator["Base"].CFrame
end
end
end
end)
end)
local Lighting = {
GS = game.Lighting.GlobalShadows,
BN = game.Lighting.Brightness,
OA = game.Lighting.OutdoorAmbient
}
Main:AddToggle("Fullbright", {flag = "FB_flag", default = false}, function(v)
_G.Fullbright = v
game:GetService("RunService").Stepped:Connect(function()
if _G.Fullbright then
game.Lighting.GlobalShadows = false
game.Lighting.Brightness = 3
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
else
game.Lighting.GlobalShadows = Lighting.GS
game.Lighting.Brightness = Lighting.BN
game.Lighting.OutdoorAmbient = Lighting.OA
end
end)
end)
Main:AddToggle("Auto Items Aura", {flag = "AIA_flag", default = false}, function(v)
_G.AutoItemsAura = v
while _G.AutoItemsAura do
game:GetService("RunService").RenderStepped:Wait(0.3)
for _,v in ipairs(workspace.CurrentRoom:GetDescendants()) do
if v.Parent.Name == "Items" and v then
fireproximityprompt(v.Prompt.ProximityPrompt)
end
end
wait(0.3)
end
end)

local Players = Tab.Players:AddSection("Players", {default = true})
Players:AddSlider("Speed", 1, 30, 25, {toggleable = true, default = false, flag = "ws_flag", fireontoggle = true, fireondrag = true, rounded = true}, function(v, v2)
_G.WS = v or 25
_G.WSE = v2
game:GetService("RunService").RenderStepped:Connect(function()
if _G.WSE and game:GetService("Players").LocalPlayer:WaitForChild("Character") then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = _G.WS
end
end)
end)
Players:AddSlider("Field Of View", 70, 120, 90, {toggleable = true, default = false, flag = "ws_flag", fireontoggle = true, fireondrag = true, rounded = true}, function(v, v2)
_G.FOV = v or 90
_G.FOVE = v2
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FOVE then
workspace.Camera.FieldOfView = _G.FOV
end
end)
end)
ESP:AddToggle("Noclip", {flag = "NC_flag", default = false}, function(v)
if v then
        noclipConnection = game:GetService("RunService").Stepped:Connect(function()
            local player = game.Players.LocalPlayer
            if player and player.Character then
                for _, part in pairs(player.Character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
    end
end)
local ESP = Tab.ESP:AddSection("ESP", {default = true})
ESP:AddToggle("Players ESP", {flag = "PE_flag", default = false}, function(v)
if v then
for _,v in ipairs(workspace.InGamePlayers:GetDescendants()) do
if v.Parent.Name == "InGamePlayers" and v then
if not v:FindFirstChild("Players_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1,1,1), 15, "Players_ESP")
end
end
end
PlayersESP = workspace.InGamePlayers.DescendantAdded:Connect(function(v)
if v.Parent.Name == "InGamePlayers" and v then
if not v:FindFirstChild("Players_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1,1,1), 15, "Players_ESP")
end
end
end)
else
PlayersESP:Disconnect()
LibraryESP:Delete("Players_ESP")
end
end)

ESP:AddToggle("Generators ESP", {flag = "GE_flag", default = false}, function(v)
if v then
for _,v in ipairs(workspace.CurrentRoom:GetDescendants()) do
if v.Parent.Name == "Generators" and v.Name == "Generator" and v.Parent and v then
if not v:FindFirstChild("Generators_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1,0,1), 15, "Generators_ESP")
end
end
end
GeneratorsESP = workspace.CurrentRoom.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Generators" and v.Name == "Generator" and v.Parent and v then
if not v:FindFirstChild("Generators_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1,0,1), 15, "Generators_ESP")
end
end
end)
else
GeneratorsESP:Disconnect()
LibraryESP:Delete("Generators_ESP")
end
end)

ESP:AddToggle("Items ESP", {flag = "IE_flag", default = false}, function(v)
if v then
for _,v in ipairs(workspace.CurrentRoom:GetDescendants()) do
if v.Parent.Name == "Items" and v.Parent and v then
if not v:FindFirstChild("Items_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(0,1), 15, "Items_ESP")
end
end
end
ItemsESP = workspace.CurrentRoom.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Items" and v.Parent and v then
if not v:FindFirstChild("Items_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1,0,1), 15, "Items_ESP")
end
end
end)
else
ItemsESP:Disconnect()
LibraryESP:Delete("Items_ESP")
end
end)

ESP:AddToggle("Monsters ESP", {flag = "ME_flag", default = false}, function(v)
if v then
for _,v in ipairs(workspace.CurrentRoom:GetDescendants()) do
if v.Parent.Name == "Monsters" and v.Parent and v then
if not v:FindFirstChild("Monsters_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1), 15, "Monsters_ESP")
end
end
end
MonstersESP = workspace.CurrentRoom.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Monsters" and v.Parent and v then
if not v:FindFirstChild("Monsters_ESP") then
LibraryESP:AddESP(v, v.Name, Color3.new(1), 15, "Monsters_ESP")
end
end
end)
else
MonstersESP:Disconnect()
LibraryESP:Delete("Monsters_ESP")
end
end)

